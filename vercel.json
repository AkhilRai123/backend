{
  "version": 2,
  "builds": [
    { "src": "/api/rooms/**", "use": "@vercel/node" },
    { "src": "/api/users/**", "use": "@vercel/node" },
    { "src": "/api/bookings/**", "use": "@vercel/node" },
    { "src": "/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/rooms/(.*)", "dest": "/api/rooms/$1" },
    { "src": "/api/users/(.*)", "dest": "/api/users/$1" },
    { "src": "/api/bookings/(.*)", "dest": "/api/bookings/$1" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
