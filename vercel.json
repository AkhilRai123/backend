{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/rooms/(.*)", "dest": "server.js" },
    { "src": "/api/users/(.*)", "dest": "server.js" },
    { "src": "/api/bookings/(.*)", "dest": "server.js" },
    { "src": "/(.*)", "dest": "/frontend/build/index.html" }
  ],
  "cleanUrls": true
}
